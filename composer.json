{
    "name": "pmg/twig-webpack",
    "description": "A Twig extension to make working with webpack a bit easier",
    "tags": ["twig", "webpack"],
    "license": "MIT",
    "require": {
        "php": "~5.6|~7.0",
        "twig/twig": "~1.27|~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "symfony/asset": "~2.8|~3.0",
        "symfony/framework-bundle": "~2.8|~3.0",
        "symfony/twig-bridge": "~2.8|~3.0",
        "symfony/phpunit-bridge": "~2.8|~3.0"
    },
    "autoload": {
        "psr-4": {
            "PMG\\TwigWebpack\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PMG\\TwigWebpack\\": "test/"
        }
    }
}
